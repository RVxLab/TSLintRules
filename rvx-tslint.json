{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [ true, "always" ],
        "quotemark": [ true, "single", "jsx-double", "avoid-escape" ],
        "interface-name": [ true, "never-prefix" ],
        "member-ordering": false,
        "no-bitwise": false,
        "arrow-parens": false,
    },
    "rulesDirectory": []
}
